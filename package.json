{
  "name": "fenertravel-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && yarn api:build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "api:dev": "tsx watch src/server/index.ts",
    "api:build": "tsc --project tsconfig.server.json && node build-server.cjs",
    "api:start": "node dist/server/server/index.cjs",
    "dev:full": "concurrently \"yarn api:dev\" \"yarn dev\"",
    "start": "yarn dev:full",
    "prod:build": "yarn build && yarn db:generate",
    "prod:start": "yarn api:start",
    "deploy:setup": "yarn install && yarn prod:build && yarn db:push",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:restart": "pm2 restart fenertravel",
    "pm2:stop": "pm2 stop fenertravel",
    "pm2:logs": "pm2 logs fenertravel",
    "pm2:status": "pm2 status"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
